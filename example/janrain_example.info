package = Janrain
core = 7.x
php = 5.3
dependencies[] = janrain
dependencies[] = janrain_widgets
dependencies[] = janrain_data
dependencies[] = rules
dependencies[] = date
dependencies[] = features
dependencies[] = text
features[features_api][] = api:2
